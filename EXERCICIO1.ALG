Algoritmo "Exercicio 1 "

Var
   altura, largura, area, perimetro: real

Inicio
   escreva ("Informe a altura em metros: ")
   leia (altura)
   escreva ("Informe a largura em metros: ")
   leia (largura)
   perimetro <- (2*largura) + (2*altura)
   area<- (altura * largura)
   escreva (" A Area do retangulo é de",area,"M2 e o perimetro é de",perimetro," metros")


Fimalgoritmo